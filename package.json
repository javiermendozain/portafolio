{
  "name": "react-pro",
  "version": "1.0.0",
  "scripts": {
    "build": "npm run clean && npm run build-client && npm run build-server",
    "build-client": "NODE_ENV=production BABEL_ENV=production node -r babel-register node_modules/.bin/webpack --progress --display-modules --config webpack/webpack.config.client.js",
    "build-server": "NODE_ENV=production BABEL_ENV=production node -r babel-register node_modules/.bin/webpack --progress --config webpack/webpack.config.server.js",
    "clean": "rimraf public/server && rimraf public/app && rimraf public/css ",
    "lint": "eslint src",
    "postmerge": "npm install",
    "postrewrite": "npm install",
    "precommit": "npm run lint",
    "prepush": "npm run test",
    "build:dev": "npm run clean & NODE_ENV=development BABEL_ENV=development babel-node src/server",
    "build:analyzer": "npm run clean && NODE_ENV=development BABEL_ENV=development ANALYZER=true babel-node src/server",
    "build:prod": "npm run build && NODE_ENV=production BABEL_ENV=production babel-node src/server",
    "deploy": "firebase deploy && cd public && git add . && git commit -m 'relase' || git push origin master",
    "test": "echo \"Error: no test specified\"",
    "watch-client": "webpack --watch --config webpack/webpack.config.client.js",
    "watch-server": "webpack --watch --config webpack/webpack.config.server.js"
  },
  "engines": {
    "node": "current"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "author": "Javier Mendoza",
  "license": "MIT",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.19",
    "@fortawesome/free-solid-svg-icons": "^5.9.0",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "bootstrap": "^4.3.1",
    "express": "^4.15.4",
    "firebase": "^6.3.1",
    "for-editor": "^0.3.0",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.26",
    "node-time-ago": "^1.0.0",
    "query-string": "^6.8.1",
    "react": "^16.8.6",
    "react-datetime": "^2.16.3",
    "react-dom": "^16.8.6",
    "react-icons": "^3.7.0",
    "react-lottie": "^1.2.3",
    "react-moment": "^0.9.2",
    "react-redux": "^7.1.0",
    "react-redux-firebase": "^2.3.0",
    "react-router-dom": "^4.4.0-beta.6",
    "react-select": "^3.0.4",
    "react-with-firebase-auth": "^1.0.3",
    "reactstrap": "^8.0.1",
    "redux": "^3.7.2",
    "redux-firestore": "^0.8.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "clean-webpack-plugin": "^0.1.16",
    "compression-webpack-plugin": "^1.0.0",
    "css-loader": "^0.28.5",
    "eslint": "^4.5.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.3.0",
    "eslint-plugin-standard": "^3.0.1",
    "extract-text-webpack-plugin": "^3.0.0",
    "husky": "^0.14.3",
    "isomorphic-style-loader": "^4.0.0",
    "node-sass": "^4.5.3",
    "open": "^6.4.0",
    "react-hot-loader": "^3.0.0-beta.7",
    "redux-devtools-extension": "^2.13.8",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "webpack": "^3.5.5",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-hot-middleware": "^2.18.2",
    "webpack-hot-server-middleware": "^0.1.0",
